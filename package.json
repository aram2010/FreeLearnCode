{
  "name": "freelearncode",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
  
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "format": "prettier --write 'src/**/*.{js,vue,css,scss,json}'",
  
    "clean": "rimraf dist",
    "analyze": "vite build --mode analyze",
  
    "deploy:gh": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^10.5.1",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "vite": "^7.1.12",
    "vite-plugin-vue-devtools": "^8.0.3"
  }
}
